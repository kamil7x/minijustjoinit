{
  "name": "minijustjoinit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "import-sort": "^6.0.0",
    "import-sort-style-react": "^5.1.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "react",
      "parser": "typescript"
    }
  }
}
